#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/sucrase@3.34.0/node_modules/sucrase/bin/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/sucrase@3.34.0/node_modules/sucrase/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/sucrase@3.34.0/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/sucrase@3.34.0/node_modules/sucrase/bin/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/sucrase@3.34.0/node_modules/sucrase/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/sucrase@3.34.0/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sucrase@3.34.0/node_modules/sucrase/bin/sucrase-node" "$@"
else
  exec node  "$basedir/../../../../../sucrase@3.34.0/node_modules/sucrase/bin/sucrase-node" "$@"
fi
