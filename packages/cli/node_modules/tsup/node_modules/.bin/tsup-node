#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules/tsup/dist/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules/tsup/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules/tsup/dist/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules/tsup/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules:/mnt/c/workspace_web/solid-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-node.js" "$@"
else
  exec node  "$basedir/../../dist/cli-node.js" "$@"
fi
